<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- This is just for example, adjust according to your requirements -->
        <include domain="sharedpref" path="." />
        <exclude domain="sharedpref" path="do_not_backup.xml" />
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="." />
        <exclude domain="sharedpref" path="device_specific.xml" />
    </device-transfer>
</data-extraction-rules>